  1     283 |C!
  1     284 |C!-----------------------OBLIQUE MERCATOR--------------------------------
  1     285 |	PARAMETER	( MCMER = 4 )
  1     286 |	PARAMETER	( MPTMER = 1, MPUTM = 2, MPOBLQ = 3 )
  1     287 |C!
  1     288 |C!-----------------------SATELLITE TRANSFORMATIONS-----------------------
  1     289 |	PARAMETER 	( MCGOES = 6 )
  1     290 |	PARAMETER 	( MPMCI = 1 )
  1     291 |C!
  1     292 |C!-----------------------COLOR TABLE INFORMATION-------------------------
  1     293 |	PARAMETER	( MXCLNM = 50 )
  1     294 |C!				Maximum number of color names saved
  1     295 |C!
  1     296 |C!-----------------------DEVICE CHARACTERISTICS--------------------------
  1     297 |	PARAMETER	( NDVCHR = 27 )
  1     298 |C!				Size of /DEVCHR/ to be passed
  1     299 |
         39 |C*
         40 |	CHARACTER*(*)	frmt, parms(*), stid
         41 |	REAL		sfdata (npgiv)
         42 |C*
         43 |	CHARACTER*8	type, cstid
         44 |	CHARACTER*1	nrth, west
         45 |	SAVE		iseq
         46 |	DATA		iseq / 0 /
         47 |	DEG ( x ) = FLOAT ( INT (x) ) + FLOAT ( MOD ( INT ( x * 100. +
         48 |     +		    .5 ), 100 ) ) / 60.
         49 |C----------------------------------------------------------------------
         50 |	iret = 0
         51 |	cstid = ' '
         52 |	CALL ST_LCUC ( frmt, type, ier )
         53 |C*
         54 |	IF ( type .eq. 'MKLEIN' ) THEN
         55 |	    READ ( lunin, *, ERR=10, END=20 ) istnm, stid, slat, slon,
         56 |     +		(sfdata (i),i=1,npgiv)
         57 |        print*,istnm,stid,slat,slon,(sfdata (i),i=1,npgiv)
         58 |C
         59 |	    selv = 0.
         60 |	    RETURN
         61 |	ELSE IF ( type .eq. 'ETAOLD' ) THEN
         62 |	    READ ( lunin, 1001, ERR=10, END=20 ) istnm, rlat, nrth,
         63 |     +					     rlon, west, selv
         64 |1001	    FORMAT (I5,1X,F5.2,A1,1X,F6.2,A1,35X,F4.0)
         65 |C*
         66 |	ELSE IF ( type .eq. 'ETA' ) THEN
         67 |	    READ ( lunin, 1201, ERR=10, END=20 ) istnm, rlat, nrth,
         68 |     +					     rlon, west, cstid, ipt,
         69 |     +					     iclas, selv
         70 |1201	    FORMAT (I6,1X,F5.2,A1,1X,F6.2,A1,1X,A4,1X,2I1,28X,F4.0)
         71 |	    sfdata ( 1 ) = istnm
         72 |	    sfdata ( 2 ) = ipt
         73 |	    sfdata ( 3 ) = iclas
         74 |	    stid = cstid
         75 |	    IF ( nrth .eq. 'S' ) THEN
         76 |		rlat = - rlat
         77 |	    END IF
         78 |	    IF ( west .eq. 'W' ) THEN
         79 |		rlon = - rlon
         80 |	    END IF
         81 |	    slat = rlat
         82 |	    slon = rlon
         83 |	    IF ( slon .gt. 180. ) slon = slon - 360.
         84 |	    RETURN
         85 |	ELSE IF ( type .eq. 'TAF' ) THEN
         86 |	    READ (lunin,1004, ERR=10, END=20 ) istnm, slat, slon,
         87 |     +		  selv, is, ifl
         88 |1004	    FORMAT ( I6,1X,F6.2,1X,F7.2,1X,F5.0,1X,I6,1X,I1 )
         89 |	    sfdata (1) = FLOAT (is)
         90 |	    sfdata (2) = FLOAT (ifl)
         91 |	    stid = ' '
         92 |	    RETURN
         93 |	ELSE IF ( type .eq. 'MRF' ) THEN
         94 |	    READ ( lunin, 1002, ERR=10, END=20 ) istnm, selv, nrth,
         95 |     +					rlat, west, rlon
         96 |1002	    FORMAT (32X,I5,2X,F4.0,2X,A1,F5.2,1X,A1,F6.2)
         97 |C
         98 |C*	    Convert deg.min to deg.hundredths.
         99 |C
        100 |	    rlon = DEG ( rlon )
        101 |	    rlat = DEG ( rlat )
        102 |C
        103 |C*	    Convert feet to meters.
        104 |C
        105 |	    selv = selv * .3048
        106 |C*
        107 |	ELSE IF ( type .eq. 'FDWIND' ) THEN
        108 |	    READ (lunin,1003,ERR=10,END=20) stid, selv, slat, slon
        109 |1003	    FORMAT ( 2X,A3,15X,I5,F6.2,F7.2 )
        110 |	    iseq = iseq + 1
        111 |	    istnm = iseq
        112 |	    sfdata ( 1 ) = istnm
        113 |	    slon = - slon
        114 |	    RETURN
        115 |	ELSE
        116 |	    iret = -20
        117 |            CALL ER_WMSG  ( 'SFFDSF', iret, 'Invalid list type. ',
        118 |     +			    ier )
        119 |
        120 |	    RETURN
        121 |	END IF
        122 |C*
        123 |	sfdata ( 1 ) = istnm
        124 |	stid = cstid
        125 |	IF ( nrth .eq. 'S' ) THEN
        126 |	    rlat = - rlat
        127 |	END IF
        128 |	IF ( west .eq. 'W' ) THEN
        129 |	    rlon = - rlon
        130 |	END IF
        131 |	slat = rlat
        132 |	slon = rlon
        133 |C**	print 232, ' STNM, Lat,Lon = ', istnm, slat, slon
        134 |C**232	format ( a,i12,', ',f12.4,', ',f12.4)
        135 |	IF ( slon .gt. 180. ) slon = slon - 360.
        136 |	RETURN
        137 |C*
        138 |10	CONTINUE
        139 |	    iret = -8
        140 |	    RETURN
        141 |20	CONTINUE
        142 |	    iret = 1
        143 |	RETURN
        144 |	END
 
